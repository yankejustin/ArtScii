<Window x:Class="ArtScii.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="650" Width="931.343">
    <Grid>
        <RichTextBox x:Name="txtAscii" HorizontalAlignment="Left" Height="539" Margin="28,45,0,0" VerticalAlignment="Top" Width="739">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
        <Button x:Name="btnCopyToClipboard" Content="Button" HorizontalAlignment="Left" Margin="802,61,0,0" VerticalAlignment="Top" Width="105" Height="61" Click="btnCopyToClipboard_Click"/>
        <Button x:Name="btnOpenOriginal" Content="Button" HorizontalAlignment="Left" Margin="802,148,0,0" VerticalAlignment="Top" Width="105" Height="61" Click="btnOpenOriginal_Click"/>
        <Button x:Name="btnSaveNewImage" Content="Button" HorizontalAlignment="Left" Margin="802,240,0,0" VerticalAlignment="Top" Width="105" Height="61" Click="btnSaveNewImage_Click"/>

    </Grid>
</Window>
