<Window x:Class="ArtScii.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="650" Width="931.343">
    <Grid>
        <RichTextBox x:Name="txtAscii" HorizontalAlignment="Left" Height="539" Margin="28,45,0,0" VerticalAlignment="Top" Width="739">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
        <Button x:Name="btnCopyToClipboard" Content="Copy to Clipboard" HorizontalAlignment="Left" Margin="781,61,0,0" VerticalAlignment="Top" Width="126" Height="60" Click="btnCopyToClipboard_Click"/>
        <Button x:Name="btnOpenOriginal" Content="Open Original Image" HorizontalAlignment="Left" Margin="781,147,0,0" VerticalAlignment="Top" Width="126" Height="60" Click="btnOpenOriginal_Click"/>
        <Button x:Name="btnSaveNewImage" Content="Save New Image" HorizontalAlignment="Left" Margin="781,238,0,0" VerticalAlignment="Top" Width="126" Height="60" Click="btnSaveNewImage_Click"/>

    </Grid>
</Window>
